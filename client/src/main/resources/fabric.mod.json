{
  "schemaVersion": 1,
  "id": "world-downloader-client",
  "version": "${version}",
  "name": "World Downloader Client",
  "description": "An addon that has a module that lets you download the world.",
  "authors": [
    "Beveled Cube"
  ],
  "contact": {
    "sources": "https://github.com/CubeBeveled/world-downloader-mod"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.beveled.wdc.WDC"
    ]
  },
  "mixins": [
    "wdc.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "35,95,210"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
